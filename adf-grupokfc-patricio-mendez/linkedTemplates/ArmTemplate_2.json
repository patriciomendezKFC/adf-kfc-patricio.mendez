{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adf-grupokfc-patricio-mendez"
		},
		"SC_CalendarizadoTallerFinal_Patricio_Mendez_properties_PL_TALLER_Patricio_Mendez_parameters_fechaCorte": {
			"type": "string",
			"defaultValue": "2020-01-11"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/SC_CalendarizadoTallerFinal_Patricio_Mendez')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Taller final",
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "PL_TALLER_Patricio_Mendez",
							"type": "PipelineReference"
						},
						"parameters": {
							"fechaCorte": "[parameters('SC_CalendarizadoTallerFinal_Patricio_Mendez_properties_PL_TALLER_Patricio_Mendez_parameters_fechaCorte')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Day",
						"interval": 1,
						"startTime": "2022-02-21T22:58:00",
						"timeZone": "SA Pacific Standard Time",
						"schedule": {
							"hours": [
								23
							]
						}
					}
				}
			},
			"dependsOn": []
		}
	]
}